{
    "manifest_version": 3,
    "name": "AI Assistant",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "storage",
      "activeTab",
      "scripting",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }